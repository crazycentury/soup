    name,\n    image,\n    active,\n  });\n}\n\nfunction setPaymentMethodStatus(payment_method_id, active) {\n  setAuthHeaderPaymentMethod();\n  return paymentMethodAxios.patch(\"/SetPaymentMethodStatus\", { payment_method_id, active });\n}\n\n\nexport { \n  getAllUsers, \n  setUserActiveStatus, \n  addUser, \n  editUser, \n  addPaymentMethod, \n  getAllPaymentMethod,\n  editPaymentMethod,\n  setPaymentMethodStatus };\n"],"mappings":"AAAA,MAAOA,MAAP,KAAkB,OAAlB,CAEA,OAASC,cAAT,KAA+B,QAA/B,CAEA,GAAMC,WAAU,CAAGF,KAAK,CAACG,MAAN,CAAa,CAC9BC,OAAO,WAAKC,OAAO,CAACC,GAAR,CAAYC,sBAAjB,WADuB,CAAb,CAAnB,CAIA,GAAMC,mBAAkB,CAAGR,KAAK,CAACG,MAAN,CAAa,CACtCC,OAAO,WAAKC,OAAO,CAACC,GAAR,CAAYC,sBAAjB,mBAD+B,CAAb,CAA3B,CAIA,QAASE,cAAT,EAAyB,CACvBP,UAAU,CAACQ,QAAX,CAAoBC,OAApB,CAA4BC,MAA5B,CAAmC,eAAnC,EAAsDX,cAAc,EAApE,CACD,CAED,QAASY,2BAAT,EAAsC,CACpCL,kBAAkB,CAACE,QAAnB,CAA4BC,OAA5B,CAAoCC,MAApC,CAA2C,eAA3C,EAA8DX,cAAc,EAA5E,CACD,CAED,QAASa,YAAT,EAAuB,CACrBL,aAAa,GACb,MAAOP,WAAU,CAACa,GAAX,CAAe,QAAf,CAAP,CACD,CAED,QAASC,oBAAT,CAA6BC,OAA7B,CAAsCC,MAAtC,CAA8C,CAC5CT,aAAa,GACb,MAAOP,WAAU,CAACiB,KAAX,CAAiB,sBAAjB,CAAyC,CAAEF,OAAO,CAAPA,OAAF,CAAWC,MAAM,CAANA,MAAX,CAAzC,CAAP,CACD,CAED,QAASE,QAAT,CAAiBC,UAAjB,CAA6BC,IAA7B,CAAmCC,KAAnC,CAA0CC,QAA1C,CAAoD,CAClDf,aAAa,GACb,MAAOP,WAAU,CAACuB,IAAX,CAAgB,UAAhB,CAA4B,CAAEJ,UAAU,CAAVA,UAAF,CAAcC,IAAI,CAAJA,IAAd,CAAoBC,KAAK,CAALA,KAApB,CAA2BC,QAAQ,CAARA,QAA3B,CAA5B,CAAP,CACD,CAED,QAASE,SAAT,CAAkBT,OAAlB,CAA2BI,UAA3B,CAAuCC,IAAvC,CAA6CC,KAA7C,CAAoDC,QAApD,CAA8D,CAC5Df,aAAa,GACb,MAAOP,WAAU,CAACiB,KAAX,CAAiB,WAAjB,CAA8B,CACnCF,OAAO,CAAPA,OADmC,CAEnCI,UAAU,CAAVA,UAFmC,CAGnCC,IAAI,CAAJA,IAHmC,CAInCC,KAAK,CAALA,KAJmC,CAKnCC,QAAQ,CAARA,QALmC,CAA9B,CAAP,CAOD,CAED,QAASG,iBAAT,CAA0BL,IAA1B,CAAgCM,KAAhC,CAAsC,CACpCf,0BAA0B,GAC1B,MAAOL,mBAAkB,CAACiB,IAAnB,CAAwB,MAAxB,CAAgC,CAACH,IAAI,CAAJA,IAAD,CAAOM,KAAK,CAALA,KAAP,CAAhC,CAAP,CACD,EAED,QAASC,oBAAT,EAA8B,CAC5B,MAAOrB,mBAAkB,CAACO,GAAnB,CAAuB,gBAAvB,CAAP,CACD,EAED,QAASe,kBAAT,CAA2BC,iBAA3B,CAA8CT,IAA9C,CAAoDM,K